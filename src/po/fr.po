#: ../libgnomecanvas/gnome-canvas-text.c:259
msgid "Font family"
msgstr "Famille de la police"

#: ../libgnomecanvas/gnome-canvas-text.c:260
msgid "Name of the font family, e.g. Sans, Helvetica, Times, Monospace"
msgstr ""
"Nom de la famille de la police, c-à-d. Sans, Helvetica, Times, Monospace"

#: ../libgnomecanvas/gnome-canvas-text.c:276
#: ../libgnomecanvas/gnome-canvas-text.c:277
msgid "Font style"
msgstr "Style de la police"

#: ../libgnomecanvas/gnome-canvas-text.c:286
#: ../libgnomecanvas/gnome-canvas-text.c:287
msgid "Font variant"
msgstr "Variante de la police"

#: ../libgnomecanvas/gnome-canvas-text.c:296
#: ../libgnomecanvas/gnome-canvas-text.c:297
msgid "Font weight"
msgstr "Graisse de la police"

#: ../libgnomecanvas/gnome-canvas-text.c:308
#: ../libgnomecanvas/gnome-canvas-text.c:309
msgid "Font stretch"
msgstr "Étirement de la police"

#: ../libgnomecanvas/gnome-canvas-text.c:318
msgid "Font size"
msgstr "Taille de la police"

#: ../libgnomecanvas/gnome-canvas-text.c:319
msgid ""
"Font size (as a multiple of PANGO_SCALE, eg. 12*PANGO_SCALE for a 12pt font "
"size)"
msgstr ""
"Taille de la police (doit être un multiple de PANGO_SCALE ; exemple : 12 "
"* PANGO_SCALE pour une police de 12 pt)"

#: ../libgnomecanvas/gnome-canvas-text.c:329
msgid "Font points"
msgstr "Points de la police"

#: ../libgnomecanvas/gnome-canvas-text.c:330
msgid "Font size in points (eg. 12 for a 12pt font size)"
msgstr ""
"Taille de la police exprimée en points (exemple : 12 pour une police 12 pt)"

#: ../libgnomecanvas/gnome-canvas-text.c:340
msgid "Rise"
msgstr "Élévation"

#: ../libgnomecanvas/gnome-canvas-text.c:341
msgid ""
"Offset of text above the baseline (below the baseline if rise is negative)"
msgstr ""
"Décalage du texte au-dessus de la ligne de base (au-dessous si l'élévation "
"est négative)"

#: ../libgnomecanvas/gnome-canvas-text.c:351
msgid "Strikethrough"
msgstr "Barré"

#: ../libgnomecanvas/gnome-canvas-text.c:352
msgid "Whether to strike through the text"
msgstr "Indique si le texte est barré"

#: ../libgnomecanvas/gnome-canvas-text.c:360
msgid "Underline"
msgstr "Souligné"

